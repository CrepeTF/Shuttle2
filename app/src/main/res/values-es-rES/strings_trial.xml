    <string name="trial_description_expired">La velocidad de reproducción aumentará una pequeña cantidad cada día, hasta que pierdas la cabeza.</string>
    <string name="trial_playback_speed">Velocidad de reproducción: 
    <xliff:g example="1.4x" id="speed">{speed}</xliff:g></string>
