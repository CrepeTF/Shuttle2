<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="tabLayoutStyle" format="reference" />

    <declare-styleable name="HomeButton">
        <attr name="type" format="enum">
            <enum name="history" value="0" />
            <enum name="recent" value="1" />
            <enum name="favorites" value="2" />
            <enum name="shuffle" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SeekButton">
        <attr name="forward" format="boolean" />
        <attr name="seconds" format="integer" />
    </declare-styleable>

    <declare-styleable name="BadgeView">
        <attr name="android:textColor" format="color|reference" />
        <attr name="android:background" format="color|reference" />
        <attr name="android:fontFamily" format="reference" />
        <attr name="android:textSize" format="dimension|reference" />
    </declare-styleable>

</resources>