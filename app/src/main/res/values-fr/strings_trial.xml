    <string name="trial_description_trial">Après l\'essai, la vitesse de lecture augmentera légèrement chaque jour, jusqu\'à ce que vous perdiez la tête.</string>
    <plurals name="trial_days_remaining">
        <item quantity="one"><xliff:g example="1" id="count">{count}</xliff:g> jour restant</item>
        <item quantity="other"><xliff:g example="30" id="count">{count}</xliff:g> jours restants</item>
    </plurals>
    <string name="trial_heading_expired">Essai expiré</string>
    <!-- Message shown when trial has expired. Note: This is meant to be friendly/fun! It doesn't have to say 'until you lose your mind' -->
    <string name="trial_description_expired">La vitesse de lecture augmentera légèrement chaque jour, jusqu\'à ce que vous perdiez la tête.</string>
    <string name="trial_playback_speed">Vitesse de lecture: <xliff:g example="1.4x" id="speed">{speed}</xliff:g></string>
    <!-- Button to upgrade from the trial version to a paid version -->
    <string name="trial_button_upgrade">Améliorer</string>
