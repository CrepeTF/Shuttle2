<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="menu_trial">Пробная версия</string>
    <string name="trial_heading_trial">Бесплатная пробная версия</string>
    <!-- Message shown when user is starting a trial. Note: This is meant to be friendly/fun! It doesn't have to say 'until you lose your mind' -->
    <string name="trial_description_trial">После пробной версии скорость воспроизведения будет немного увеличиваться каждый день, пока не достигнет умопомрачительных величин.</string>
    <plurals name="trial_days_remaining">
