<?xml version="1.0"?>
<resources>
    <!-- 'Loading' text. Shown on various screens as their content is loading-->
    <string name="loading">Wird geladen…</string>
    <!-- Shown in various places to indicate this particular field is unknown. E.g. the album name is 'unknown', or the artist is 'unknown' -->
    <string name="unknown">Unbekannt</string>
    <!-- Shown when the playback queue is empty -->
    <string name="queue_empty">Warteschlange ist leer</string>
    <!-- Shown when the current playback queue is empty. Pressing the widget will just open the app. -->
    <string name="widget_empty_text">Wählen Sie ein Song aus…</string>
</resources>
